<?xml version="1.0" ?>

<launch>

  <node pkg="arox_docking" type="detect_container_entry.py" name="arox_docking" respawn="true" output="screen"></node>

  <node pkg="pointcloud_to_laserscan" type="pointcloud_to_laserscan_node" name="pointcloud_to_laserscan" respawn="true" output="screen">
    <rosparam>
      target_frame: base_link
      transform_tolerance: 0.01
      min_height: 0.3
      max_height: 2.0

      angle_min: -6.28
      angle_max: 6.28
      angle_increment: 0.0001

      scan_time: 0.3333
      range_min: 0.45
      range_max: 15.0
      use_inf: true
      concurrency_level: 0
    </rosparam>

    <remap from="scan" to="scan_velodyne" />
    <remap from="cloud_in" to="velodyne_points" />
  </node>

</launch>
